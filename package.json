{
	"name": "cablesGL",
	"scripts": {
		"start": "serve -s patch"
		"postinstall": "electron-builder install-app-deps",
		"build": "electron-builder --mac --windows --linux",
		"release": "electron-builder --mac --windows --linux --publish always"
	},
	"build": {
		"appId": "com.jaa09.cablesGL",
		"productName": "Cables GL",
		"mac": {
			"category": "public.app-category.lifestyle"
		},
		"dmg": {
			"icon": false
		},
		"linux": {
			"target": ["AppImage"],
			"category": "Office"
		}
	},
	"dependencies": {
		"serve": "^13.0.2",
		"electron-builder": "^14.0.0"
	},
	"devDependencies": {
		"serve": "^13.0.2",
		"electron-builder": "^14.0.0"
	}
}
